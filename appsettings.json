{
  "AppConfiguration": {
    "RedisConfiguration": "redis-service.default.svc.cluster.local:6379",
    "PostgresqlConfiguration": "Host=postgresql-service.default.svc.cluster.local;Port=5432;Username=postgres;Password=admin123;Database=weatherdb;"
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft": "Warning"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          //"path": "D://Serilogs//Applogs.log",
          "path": "/var/log/applogs.log"
        }
      }
    ]
  }
}


